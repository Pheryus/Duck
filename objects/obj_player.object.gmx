        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Get the player's input

key_right = keyboard_check(ord('D'));
key_left =  -keyboard_check(ord('A'));
key_restart = keyboard_check(vk_alt);
//key_jump = keyboard_check_pressed(vk_space);

if(key_restart){
    game_restart();
}

//React to inputs

move = key_left + key_right;
hsp = move * movespeed;

//gravity
if( vsp  &lt; 10)
    vsp += grav;

//Horizontal Collision
if(place_meeting(x+hsp,y,obj_wall)){
    while(!place_meeting(x+sign(hsp),y,obj_wall)){
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

//Vertical Collision
if(place_meeting(x,y+vsp,obj_wall)){
    while(!place_meeting(x,y+sign(vsp),obj_wall)){
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

//Horizontal Collision Player
if(place_meeting(x+hsp+1,y,obj_player2)){
    hsp = 0;
}
//Vertical Collision
if(place_meeting(x,y+vsp+1,obj_player2)){
    vsp = 0;
}

if(place_meeting(x,y+vsp,obj_wall2) or place_meeting(x+hsp,y,obj_wall2)){
    global.numenemy = 0;
    room_restart();
}






//Needle Collision
if(place_meeting(x+hsp,y,obj_needle)){
    while(!place_meeting(x+sign(hsp),y,obj_needle)){
        x += sign(hsp);
    }
    hsp = 0;
}



</string>
